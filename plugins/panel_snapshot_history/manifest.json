{
    "id": "panel_snapshot_history",
    "name": "Snapshot History Panel",
    "version": "1.0.0",
    "description": "Provides a tree-based UI to visualize and switch between sandbox snapshots.",
    "author": "Hevno Team",
    "frontend": {
        "type": "cockpit-panel",
        "entryPoint": "dist/main.js",
        "srcEntryPoint": "src/SnapshotHistoryPanel.jsx",
        "contributions": {
            "cockpit.panels": [
                {
                    "id": "panel_snapshot_history.main",
                    "name": "快照历史",
                    "componentExportName": "SnapshotHistoryPanel",
                    "defaultState": { "x": 20, "y": 20, "width": 400, "height": 600 },
                    "defaultEnabled": true
                }
            ],
            "cockpit.chrome_actions": [
                {
                    "id": "panel_snapshot_history.toggle_button",
                    "panelId": "panel_snapshot_history.main",
                    "title": "切换快照历史",
                    "icon": "History"
                }
            ]
        }
    }
}